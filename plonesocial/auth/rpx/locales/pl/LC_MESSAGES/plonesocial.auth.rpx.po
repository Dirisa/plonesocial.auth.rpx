#: ./browser/configlet.py:45
msgid "Enter your RPX api key"
msgstr "Wpisz swój klucz api RPX"

#: ./skins/rpx_skins/login_form_validate.vpy:47
#: ./skins/rpx_skins/rpx_logged_in.cpy:19
msgid "Login failed. Both login name and password are case sensitive, check that caps lock is not enabled."
msgstr "Logowanie nieudane. Zarówno nazwa użytkownika jak i hasło są wrażliwe na wielkość liter, upewnij się, że nie masz włączonej funkcji Caps Lock na klawiaturze"

#: ./skins/rpx_skins/login_form_validate.vpy:44
msgid "Login name not found."
msgstr "Niepoprawna nazwa użytkownika."

#: ./skins/rpx_skins/login_form_validate.vpy:46
msgid "Password incorrect."
msgstr "Niepoprawne hasło."

#: ./skins/rpx_skins/join_form_validate.vpy:17
msgid "Passwords do not match."
msgstr "Podane hasła nie są identyczne."

#: ./skins/rpx_skins/join_form_validate.vpy:19
msgid "Passwords must contain at least 5 letters."
msgstr "Hasło musi zawierać co najmniej 5 znaków."

#: ./skins/rpx_skins/join_form_validate.vpy:59
#: ./skins/rpx_skins/register.cpy:37
msgid "Please correct the indicated errors."
msgstr "Proszę poprawić zaznaczone błędy."

#: ./skins/rpx_skins/login_form_validate.vpy:37
msgid "Please enter your login name."
msgstr "Podaj swoją nazwę użytkownika."

#: ./skins/rpx_skins/login_form_validate.vpy:39
msgid "Please enter your password."
msgstr "Podaj swoje hasło."

#: ./browser/configlet.py:46
msgid "RPX Settings"
msgstr "Ustawienia RPX"

#: ./skins/rpx_skins/rpx_logged_in.cpy:40
msgid "The RPX id was mapped to your user."
msgstr "Twoje ID zostało przypisane do twojego użytkownika."

#: ./skins/rpx_skins/join_form_validate.vpy:48
#: ./skins/rpx_skins/register.cpy:36
msgid "The login name you selected is already in use or is not valid. Please choose another."
msgstr "Podana nazwa użytkownika jest już w użyciu lub jest nieprawidłowa. Proszę podać inną."

#: ./skins/rpx_skins/join_form_validate.vpy:56
msgid "This RPX-identifier has already been mapped to a different user."
msgstr "To ID jest już przypisane do innego użytkownika."

#: ./skins/rpx_skins/join_form_validate.vpy:15
msgid "This field is required, please provide some information."
msgstr "To pole jest wymagane."

#: ./skins/rpx_skins/join_form_validate.vpy:21
msgid "This username is reserved. Please choose a different name."
msgstr "Podana nazwa użytkownika jest zajęta, proszę wybrać inną."

#: ./skins/rpx_skins/register.cpy:69
msgid "User added."
msgstr "Użytkownik został utworzony."

#: ./skins/rpx_skins/login_form_validate.vpy:21
msgid "You must enable cookies before you can log in."
msgstr "Musisz włączyć obsługę plików cookie żeby móc się zalogować."

#: ./skins/rpx_skins/personalize.cpy:69
msgid "Your personal settings have been saved."
msgstr "Twoje ustawienia osobiste zostały zapisane."

#. Default: "Sign In"
#: ./portlets/rpxportlet.pt:20
msgid "rpx_portlet_signin"
msgstr "Zaloguj się"

#. Default: "Failed to create your account: we were unable to send your password to your email address: ${address}"
#: ./skins/rpx_skins/register.cpy:55
msgid "status_fatal_password_mail"
msgstr "Nie udało się utworzyć konta, ponieważ nie można wysłać hasła na adres email: ${address}"

#. Default: "You account has been created, but we were unable to send your password to your email address: ${address}"
#: ./skins/rpx_skins/register.cpy:61
msgid "status_nonfatal_password_mail"
msgstr "Twoje konto zostało utworzone, ale nie udało się wysłać Twojego hasła na adres email: ${address}"

